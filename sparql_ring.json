{
  "name": "OKN",
  "description": "Open Knowledge Network Graph Data",
  "graphs": {
    "PACER": {
      "description": "PACER data from the federal court system",
      "url": "http://natchez.chem-eng.northwestern.edu:3030/scales/sparql",
      "prefixes": [
        "PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>",
        "PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>",
        "PREFIX scales: <http://schemas.scales-okn.org/rdf/scales#>"
      ],
      "multiValueDelimiter": "|",
      "dateRangeDelimiter": ",",
      "fields": {
        "docketId": {
          "type": "string",
          "name": "Case",
          "description": "The parent case",
          "predicate": "scales:hasPacerID",
          "required": true,
          "canFilter": true
        },
        "caseType": {
          "type": "string",
          "name": "Case Type",
          "description": "The type of case",
          "predicate": "a",
          "required": true,
          "canFilter": true
        },
        "filingDate": {
          "type": "date-time",
          "name": "Filing Date",
          "description": "The date the case was filed",
          "predicate": "scales:hasIdbFiledate",
          "required": true,
          "canFilter": true
        },
        "terminatingDate": {
          "type": "date",
          "name": "Terminating Date",
          "description": "The date the case terminated",
          "predicate": "scales:hasTerminatingDate",
          "required": true,
          "canFilter": true
        },
        "court": {
          "type": "string",
          "name": "Court",
          "description": "The court where the case was filed",
          "predicate": "scales:isInCourt",
          "required": true
        },
        "courtName": {
          "type": "string",
          "name": "Court Name",
          "description": "The name of the court",
          "predicate": "scales:hasName",
          "required": true,
          "parent": "court",
          "canFilter": true
        },
        "courtCircuit": {
          "type": "string",
          "name": "Court Circuit",
          "description": "The circuit of the court",
          "predicate": "scales:isInCircuit",
          "required": true,
          "parent": "court",
          "canFilter": true
        },
        "natureOfSuit": {
          "type": "string",
          "name": "Nature of Suit",
          "description": "The nature of the suit",
          "predicate": "scales:hasNatureSuit",
          "required": false,
          "canFilter": true
        },
        "docketTable": {
          "type": "string",
          "name": "Docket Table",
          "description": "Table that holds the docket entries",
          "predicate": "scales:hasDocketTable",
          "required": false,
          "canFilter": false
        },
        "docketEntry": {
          "type": "string",
          "name": "Docket Entry",
          "description": "The docket entry",
          "predicate": "?p",
          "required": false,
          "parent": "docketTable",
          "canFilter": false
        },
        "ontologyLabel": {
          "type": "iri",
          "iriPrefix": "http://schemas.scales-okn.org/rdf/scales#OntologyLabel",
          "name": "Ontology Label",
          "description": "The label of the ontology",
          "predicate": "scales:hasOntologyLabel",
          "required": false,
          "parent": "docketEntry",
          "canFilter": true
        }
      }
    }
  }
}