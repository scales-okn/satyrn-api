{
  "name": "OKN",
  "description": "Open Knowledge Network MongoDB Data",
  "mongo": {
    "PACER": {
      "description": "PACER data from the federal court system",
      "url": "http://natchez.chem-eng.northwestern.edu:3030/scales/sparql",
      "prefixes": [
        "PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>",
        "PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>",
        "PREFIX scales: <http://schemas.scales-okn.org/rdf/scales#>"
      ],
      "fields": {
        "case_name": {
          "name": "Case Name",
          "description": "The parent case",
          "predicate": "a",
          "optional": false,
          "parent": "case",
          "variable": "caseType",
          "canFilter": true
        },
        "case_id": {
          "name": "Case ID",
          "description": "The parent case",
          "predicate": "a",
          "optional": false,
          "parent": "case",
          "variable": "caseType",
          "canFilter": true
        },
        "filing_date": {
          "name": "Filing Date",
          "description": "The date the case was filed",
          "predicate": "scales:hasFilingDate",
          "optional": false,
          "parent": "case",
          "canFilter": true
        },
        "terminating_date": {
          "name": "Terminating Date",
          "description": "The date the case terminated",
          "predicate": "scales:hasTerminatingDate",
          "optional": false,
          "parent": "case",
          "canFilter": true
        },
        "court": {
          "name": "Court",
          "description": "The court where the case was filed",
          "predicate": "scales:isInCourt",
          "optional": false,
          "parent": "case",
          "canFilter": true
        },
        "judge": {
          "name": "Judge",
          "description": "The judge presiding over the case",
          "predicate": "scales:isInCourt",
          "optional": false,
          "parent": "case",
          "canFilter": true
        },
        "nature_suit": {
          "name": "Nature of Suit",
          "description": "The nature of the suit",
          "predicate": "scales:hasNatureSuit",
          "optional": true,
          "parent": "case",
          "canFilter": true
        }
      }
    }
  }
}
